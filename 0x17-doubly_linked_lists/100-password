#include <stdio.h>

int is_number_palindrome(int n);

int main(void)
{
	int i = 0, j = 0;
	for (i = 100; i < 999; i++)
	{
		for (j = 100; j < 999; j++)
		{
			if(is_number_palindrome(i) && is_number_palindrome(j) && is_number_palindrome(i*j))
				printf("%d %d %d\n", i, j , (i*j));

		}
	}
}


int is_number_palindrome(int n)
{
	int reversed = 0, remainder, original;
	original = n;
	
	while (n != 0) {
		remainder = n % 10;
		reversed = reversed * 10 + remainder;
		n /= 10;
	}
	if (original == reversed)
		return 1;
	else
		return 0;
}
